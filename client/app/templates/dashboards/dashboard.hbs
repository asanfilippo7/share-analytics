<div class='container-fluid'>
    <div class='row'>
        <div class='col-md-12'>
            <h2>{{dashboardName}}</h2>
        </div>
    </div>
</div>

<div id='freewall' class='free-wall {{wrapperClass}}'>
    {{#each widgets as |item|}}
        {{widget-adapter options=addableList addChart=(action "addChart") removeChart=(action 'removeChart') item=item widgets=widgets wall=wall refreshWall=(action 'refreshWall') dashboardSaveWidget=(action 'dashboardSaveWidget') parameters=parameters}}
    {{/each}}
</div>

